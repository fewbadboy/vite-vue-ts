{
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-var": "error",
    "eqeqeq": ["error", "always", { "null": "ignore" }],
    "curly": ["error", "all"]
  },
  "extends": [
    ".oxlint-typescript.json"
  ],
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "ignorePatterns": [
    "node_modules/",
    "dist/",
    "public/",
    "src/icons/svg/"
  ]
}