{
  "name": "vite-vue-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server": "nodemon mock/index.mjs",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.config.cjs",
    "prepare": "husky",
    "oxlint": "oxlint . --fix --fix-suggestions --fix-dangerously",
    "lint-staged": "lint-staged",
    "addition-commit": "git add . && git commit --amend --no-edit"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.10.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-plus": "^2.10.2",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.7",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@faker-js/faker": "^9.8.0",
    "@tailwindcss/vite": "^4.1.11",
    "@types/lodash": "^4.17.19",
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "chokidar": "^4.0.3",
    "express": "^5.1.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "multer": "^2.0.1",
    "nodemon": "^3.1.10",
    "oxlint": "^1.4.0",
    "picocolors": "^1.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "rollup-plugin-visualizer": "^6.0.3",
    "svgo": "3.3.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.0",
    "vite-plugin-oxlint": "^1.3.3",
    "vite-plugin-svg-icons-ng": "^1.4.1",
    "vue-tsc": "^2.2.10"
  },
  "repository": {
