    "nprogress": "^0.2.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.23",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@faker-js/faker": "^10.1.0",
