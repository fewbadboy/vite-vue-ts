{
  "plugins": [
    {
      "name": "typescript",
      "options": {
        "tsconfig": "./tsconfig.json"
      }
    }
  ],
  "rules": {
    "typescript/no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "ignoreRestSiblings": true,
        "varsIgnorePattern": "^_"
      }
    ],
    "typescript/no-explicit-any": "error"
  }
}